{
  "name": "api-solid",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev:server": "tsx watch src/shared/infra/http/server.ts",
    "start": "node build/server.js",
    "build": "tsup src --out-dir build",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "test:create-prisma-environment": "sudo npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "sudo npm link vitest-environment-prisma",
    "test": "vitest run src/**/use-cases/**",
    "test:watch": "vitest run src/**/use-cases/**",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "test:e2e": "vitest run src/**/infra/http/controllers/**",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@fastify/jwt": "7.2.1",
    "@prisma/client": "4.16.2",
    "bcryptjs": "2.4.3",
    "dayjs": "1.11.9",
    "dotenv": "16.3.1",
    "fastify": "4.19.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "8.0.2",
    "@rocketseat/eslint-config": "1.2.0",
    "@types/bcryptjs": "2.4.2",
    "@types/node": "20.3.3",
    "@types/supertest": "2.0.12",
    "@vitest/coverage-v8": "0.33.0",
    "@vitest/ui": "0.33.0",
    "eslint": "8.44.0",
    "npm-run-all": "4.1.5",
    "prisma": "4.16.2",
    "supertest": "6.3.3",
    "tsup": "7.1.0",
    "tsx": "3.12.7",
    "typescript": "5.1.6",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.33.0"
  }
}
